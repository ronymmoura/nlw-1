{
    "name": "server",
    "version": "0.0.1",
    "main": "index.js",
    "author": "Rony Moura <ronymmoura@gmail.com>",
    "license": "MIT",
    "scripts": {
        "start": "node dist/server.js",
        "dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpileOnly --ignore-watch=node_modules --no-notify src/server.ts",
        "lint": "./node_modules/.bin/eslint",
        "test": "jest --testTimeout=10000 --detectOpenHandles",
        "build": "babel src --extensions \".js,.ts\" --out-dir dist --copy-files --no-copy-ignored",
        "knex:migrate": "knex migrate:latest --knexfile knexfile.ts",
        "knex:seed": "knex seed:run --knexfile knexfile.ts"
    },
    "devDependencies": {
        "@babel/cli": "^7.10.3",
        "@babel/core": "^7.10.3",
        "@babel/node": "^7.10.3",
        "@babel/preset-env": "^7.10.3",
        "@babel/preset-typescript": "^7.10.1",
        "@types/cors": "^2.8.6",
        "@types/express": "^4.17.6",
        "@types/hapi__joi": "^17.1.2",
        "@types/jest": "^26.0.0",
        "@types/multer": "^1.4.3",
        "@typescript-eslint/eslint-plugin": "^3.3.0",
        "@typescript-eslint/parser": "^3.3.0",
        "babel-plugin-module-resolver": "^4.0.0",
        "cross-env": "^7.0.2",
        "eslint": "^7.3.0",
        "eslint-config-standard": "^14.1.1",
        "eslint-plugin-import": "^2.21.2",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^4.2.1",
        "eslint-plugin-standard": "^4.0.1",
        "jest": "^26.0.1",
        "supertest": "^4.0.2",
        "ts-jest": "^26.1.0",
        "ts-node": "^8.10.2",
        "ts-node-dev": "^1.0.0-pre.44",
        "tsconfig-paths": "^3.9.0",
        "typescript": "^3.9.5"
    },
    "dependencies": {
        "@types/supertest": "^2.0.9",
        "celebrate": "^12.1.1",
        "cors": "^2.8.5",
        "express": "^4.17.1",
        "knex": "^0.21.1",
        "multer": "^1.4.2",
        "sqlite3": "^4.2.0"
    }
}
